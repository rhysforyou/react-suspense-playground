{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "modules": false,
        "exclude": ["transform-typeof-symbol"],
        "targets": ["last 2 versions", "> 2%", "not dead"]
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/syntax-dynamic-import"
  ]
}
